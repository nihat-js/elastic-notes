{
  "manifest_version": 3,
  "name": "Elastic Notes",
  "version": "1.0",
  "description": "Elastic Notes created By Nihat",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "commands": {
    "invokeFunction": {
        "suggested_key": {
            "default": "Ctrl+Shift+A",
            "mac": "Command+Shift+A"
        },
        "description": "Invoke Function"
    }
}
}